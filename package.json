{
  "name": "utility-sdk",
  "version": "1.0.5",
  "description": "A lightweight TypeScript utility SDK",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "watch": "tsc -w",
    "start": "ts-node sandbox/sandbox.ts",
    "dev": "ts-node-dev --respawn sandbox/sandbox.ts"
  },
  "keywords": [
    "simple",
    "utility",
    "sdk"
  ],
  "author": "rpy.jp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JP-Repo-07/utility-sdk.git"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "crypto": "^1.0.1",
    "jsonwebtoken": "^9.0.2"
  }
}
